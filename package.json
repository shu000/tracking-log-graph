{
  "name": "tracking-log-graph",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx npm-run-all -s build run:server",
    "build": "npx npm-run-all -p build:sass build:js",
    "watch": "npx npm-run-all -p watch:js watch:sass",
    "watch:js": "npx webpack-dev-server --config ./webpack.config.js --open --mode development",
    "watch:sass": "npx node-sass -w ./src/client/css/index.scss ./dist/index.css",
    "build:sass": "npx npm-run-all -p build:sass:index build:sass:bootstrap",
    "build:sass:index": "npx node-sass ./src/client/css/index.scss ./dist/index.css",
    "build:sass:bootstrap": "cp ./src/client/css/bootstrap.css ./dist/bootstrap.css",
    "build:js": "npx webpack --config ./webpack.config.js --mode development",
    "run:server": "node ./server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-fetch": "^3.0.4",
    "express": "^4.17.1",
    "mongo-sanitize": "^1.0.1",
    "mongodb": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  }
}
